  dialogueObject:
  - currentDialogueLine: 0
    dialogue: Good job, finding those crystals! Now give them to me!
    nextLine:
    - playerResponses: '@player gives @npc the crystals'
      nextDialogueLine: 1
    _dialoguetype: 1
    _triggerEvent: 0
    audio: {fileID: 0}
  - currentDialogueLine: 1
    dialogue: '@npc pulls out some sort of device and inserts the crystals, one by
      one, into it. The crystals seem to fade in colour and the device is emitting
      more and more light, every time he drains a crystal.'
    nextLine:
    - playerResponses: Continue.
